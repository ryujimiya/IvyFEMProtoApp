<Window x:Class="IvyFEMProtoApp.ChartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IvyFEMProtoApp"
        xmlns:oxy="http://oxyplot.org/wpf"
        mc:Ignorable="d"
        Title="ChartWindow" Height="340" Width="300">
    <Grid>
        <oxy:PlotView x:Name="Plot" Model="{Binding PlotModel}" Margin="0,0,0,28"/>
        <TextBox x:Name="TextBox1" TextWrapping="Wrap" Text="" IsReadOnly="True" Height="28" VerticalAlignment="Bottom"/>
    </Grid>
</Window>
